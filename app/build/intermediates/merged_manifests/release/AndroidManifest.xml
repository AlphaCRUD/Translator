            android:exported="false" />
        <activity
            android:name="com.alphacrud.phototranslator.ShareActivity"
            android:exported="false" />
        <activity
            android:name="com.alphacrud.phototranslator.OCRActivity"
            android:exported="false" />
        <activity
            android:name="com.alphacrud.phototranslator.CameraX"
            android:exported="false" />
        <activity
            android:name="com.alphacrud.phototranslator.CameraKit"
            android:exported="false" />
