            android:name="com.alphacrud.phototranslator.Adds"
            android:exported="false"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.alphacrud.phototranslator.privacy_policy"
            android:exported="false"
            android:hardwareAccelerated="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.alphacrud.phototranslator.ScanResult"
